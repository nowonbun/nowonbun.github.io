<?xml version="1.0" encoding="utf-8"?>
<skin>
	<information>
		<name>Tirium</name>
		<version>0.83b</version>
		<description>
		<![CDATA[ Tirium is medium-like theme for Tistory designed by Vanns Kang ]]>
		</description>
		<license>
		<![CDATA[GNU General Public License. GPL. HOWEVER, Must Notify about Author's info ]]>
		</license>
	</information>
	<author>
		<name>Vanns Kang</name>
		<homepage>http://vannskang.tistory.com</homepage>
		<email>vannskang@gmail.com</email>
	</author>
	<default>
		<recentEntries>0</recentEntries>
		<recentComments>0</recentComments>
		<recentTrackbacks>0</recentTrackbacks>
		<itemsOnGuestbook>0</itemsOnGuestbook>
		<tagsInCloud>30</tagsInCloud>
		<sortInCloud>1</sortInCloud>
		<expandComment>1</expandComment>
		<expandTrackback>1</expandTrackback>
		<lengthOfRecentNotice>30</lengthOfRecentNotice>
		<lengthOfRecentEntry>30</lengthOfRecentEntry>
		<lengthOfRecentComment>10</lengthOfRecentComment>
		<lengthOfRecentTrackback>10</lengthOfRecentTrackback>
		<lengthOfLink>30</lengthOfLink>
		<showListOnCategory>1</showListOnCategory>
		<showListOnArchive>1</showListOnArchive>
		<commentMessage>
			<none>댓글이 없습니다.</none>
			<single>댓글 &lt;span class="cnt"&gt;하나&lt;/span&gt; 달렸습니다.</single>
		</commentMessage>
		<trackbackMessage>
			<none>받은 트랙백이 없고</none>
			<single>트랙백이 &lt;span class="cnt"&gt;하나&lt;/span&gt;이고</single>
		</trackbackMessage>
		<tree>
			<color>000000</color>
			<bgColor>ffffff</bgColor>
			<activeColor>000000</activeColor>
			<activeBgColor>eeeeee</activeBgColor>
			<labelLength>15</labelLength>
			<showValue>0</showValue>
			</tree>
		<contentWidth>700</contentWidth>
	</default>
